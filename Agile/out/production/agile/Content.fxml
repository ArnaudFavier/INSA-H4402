<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import com.jfoenix.controls.JFXTreeTableColumn?>
<?import com.jfoenix.controls.JFXTreeTableView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<StackPane fx:id="root" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
	<VBox spacing="10">
		<children>
			<!-- <JFXTreeTableView fx:id="livraisonTableView" prefHeight="470.0" prefWidth="800.0" /> -->
			<HBox alignment="CENTER" prefWidth="800.0">
				<Label minWidth="180.0" styleClass="title-label" text="Liste des livraisons">Read Only Tree Table View</Label>
				<Label fx:id="treeTableViewCount" minWidth="150.0" styleClass="title-label" />
				<JFXButton fx:id="treeTableViewAdd" buttonType="RAISED" style="-fx-background-color: WHITE;" text="+">
               <HBox.margin>
                  <Insets left="5.0" right="5.0" />
               </HBox.margin></JFXButton>
				<JFXButton fx:id="treeTableViewRemove" buttonType="RAISED" style="-fx-background-color: WHITE;" text="-">
               <HBox.margin>
                  <Insets left="5.0" right="5.0" />
               </HBox.margin>
            </JFXButton>
				<StackPane prefWidth="556.0" HBox.hgrow="ALWAYS"><StackPane maxWidth="200" StackPane.alignment="CENTER_RIGHT"><JFXTextField fx:id="searchField" promptText="Rechercher..." /></StackPane></StackPane>
			</HBox>
			<JFXTreeTableView fx:id="livraisonTreeTableView" prefWidth="800.0">
				<columns>
					<JFXTreeTableColumn fx:id="colonneAdresse" maxWidth="1.7976931348623157E308" minWidth="160.0" prefWidth="160.0" resizable="false" text="Adresse" />
					<JFXTreeTableColumn fx:id="colonneDuree" maxWidth="1.7976931348623157E308" minWidth="105.0" prefWidth="105.0" resizable="false" text="Duree" />
					<JFXTreeTableColumn fx:id="colonnePlageDebut" maxWidth="1.7976931348623157E308" minWidth="105.0" prefWidth="105.0" resizable="false" text="Plage début" />
               <JFXTreeTableColumn fx:id="colonnePlageFin" maxWidth="1.7976931348623157E308" minWidth="105.0" prefWidth="105.0" resizable="false" text="Plage Fin" />
               <JFXTreeTableColumn fx:id="colonneHeureArrivee" maxWidth="1.7976931348623157E308" minWidth="105.0" prefWidth="105.0" resizable="false" text="Heure arrivé" />
               <JFXTreeTableColumn fx:id="colonneHeureDepart" maxWidth="1.7976931348623157E308" minWidth="105.0" prefWidth="105.0" resizable="false" text="Heure départ" />
               <JFXTreeTableColumn fx:id="colonneTempsAttente" maxWidth="1.7976931348623157E308" minWidth="105.0" prefWidth="105.0" resizable="false" text="Temps attente" />
				</columns>
			</JFXTreeTableView>
			<HBox prefHeight="100.0" prefWidth="200.0">
				<children>
					<JFXButton buttonType="RAISED" onAction="#boutonOuvrirPlan" prefHeight="40" prefWidth="130.0" style="-fx-font-size:14px;-fx-background-color:WHITE;" text="Ouvrir plan">
						<cursor>
							<Cursor fx:constant="HAND" />
						</cursor>
						<font>
							<Font name="Arial" size="12.0" />
						</font>
						<HBox.margin>
							<Insets left="10.0" right="10.0" />
						</HBox.margin>
					</JFXButton>
					<JFXButton buttonType="RAISED" onAction="#boutonOuvrirLivraison" prefHeight="40" prefWidth="130.0" style="-fx-font-size:14px;-fx-background-color:WHITE;" text="Ouvrir livraison">
						<cursor>
							<Cursor fx:constant="HAND" />
						</cursor>
						<font>
							<Font name="Arial" size="12.0" />
						</font>
						<HBox.margin>
							<Insets left="10.0" right="10.0" />
						</HBox.margin>
					</JFXButton>
					<JFXButton buttonType="RAISED" onAction="#boutonCalculerTournee" prefHeight="40" prefWidth="130.0" style="-fx-font-size:14px;-fx-background-color:WHITE;" text="Calculer tournée">
						<cursor>
							<Cursor fx:constant="HAND" />
						</cursor>
						<font>
							<Font name="Arial" size="12.0" />
						</font>
						<HBox.margin>
							<Insets left="10.0" right="10.0" />
						</HBox.margin>
					</JFXButton>
					<JFXButton buttonType="RAISED" onAction="#boutonExporterTournee" prefHeight="40" prefWidth="130.0" style="-fx-font-size:14px;-fx-background-color:WHITE;" text="Exporter tournée">
						<cursor>
							<Cursor fx:constant="HAND" />
						</cursor>
						<font>
							<Font name="Arial" size="12.0" />
						</font>
						<HBox.margin>
							<Insets left="10.0" right="10.0" />
						</HBox.margin>
					</JFXButton>
				</children>
			</HBox>
		</children>
	</VBox>
</StackPane>
