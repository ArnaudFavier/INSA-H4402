<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import com.jfoenix.controls.JFXTreeTableColumn?>
<?import com.jfoenix.controls.JFXTreeTableView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<StackPane fx:id="root" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
	<VBox spacing="10">
		<children>
         <HBox alignment="CENTER" layoutX="10.0" layoutY="275.0" prefWidth="800.0">
            <children>
               <Label minWidth="180.0" styleClass="title-label" text="Entrepôt" />
               <StackPane prefWidth="556.0" HBox.hgrow="ALWAYS" />
            </children>
         </HBox>
         <JFXTreeTableView fx:id="entrepotTreeTableView" prefHeight="160.0" prefWidth="800.0">
            <columns>
               <JFXTreeTableColumn fx:id="colonneEntrepotAdresse" editable="false" maxWidth="1.7976931348623157E308" minWidth="200.0" prefWidth="200.0" resizable="false" sortable="false" text="Adresse" />
               <JFXTreeTableColumn fx:id="colonneEntrepotHeureDepart" editable="false" maxWidth="1.7976931348623157E308" minWidth="200.0" prefWidth="200.0" resizable="false" sortable="false" text="Heure départ" />
            </columns>
         </JFXTreeTableView>
			<HBox alignment="CENTER" prefWidth="800.0">
				<Label minWidth="180.0" styleClass="title-label" text="Liste des livraisons" />
				<Label fx:id="treeTableViewCount" minWidth="150.0" styleClass="title-label" />
				<JFXButton fx:id="treeTableViewAdd" buttonType="RAISED" minWidth="100.0" style="-fx-background-color: WHITE;" text="Ajouter">
               <HBox.margin>
                  <Insets left="5.0" right="5.0" />
               </HBox.margin></JFXButton>
				<JFXButton fx:id="treeTableViewRemove" buttonType="RAISED" minWidth="100.0" style="-fx-background-color: WHITE;" text="Supprimer">
               <HBox.margin>
                  <Insets left="5.0" right="5.0" />
               </HBox.margin>
            </JFXButton>
				<StackPane prefWidth="556.0" HBox.hgrow="ALWAYS"><StackPane maxWidth="200" StackPane.alignment="CENTER_RIGHT"><JFXTextField fx:id="searchField" promptText="Rechercher..." /></StackPane></StackPane>
			</HBox>
			<JFXTreeTableView fx:id="livraisonTreeTableView" editable="true" prefHeight="220.0" prefWidth="800.0">
				<columns>
					<JFXTreeTableColumn fx:id="colonneAdresse" editable="false" maxWidth="1.7976931348623157E308" minWidth="125.0" prefWidth="160.0" resizable="false" sortable="false" text="Adresse" />
               <JFXTreeTableColumn fx:id="colonneHeureArrivee" editable="false" maxWidth="1.7976931348623157E308" minWidth="125.0" prefWidth="105.0" resizable="false" sortable="false" text="Heure arrivée" />
               <JFXTreeTableColumn fx:id="colonneTempsAttente" editable="false" maxWidth="1.7976931348623157E308" minWidth="125.0" prefWidth="105.0" resizable="false" sortable="false" text="Temps attente" />
					<JFXTreeTableColumn fx:id="colonneDuree" editable="false" maxWidth="1.7976931348623157E308" minWidth="125.0" prefWidth="105.0" resizable="false" sortable="false" text="Durée" />
               <JFXTreeTableColumn fx:id="colonneHeureDepart" editable="false" maxWidth="1.7976931348623157E308" minWidth="125.0" prefWidth="105.0" resizable="false" sortable="false" text="Heure départ" />
					<JFXTreeTableColumn fx:id="colonnePlagePrevisionnelle" maxWidth="1.7976931348623157E308" minWidth="125.0" prefWidth="105.0" resizable="false" sortable="false" text="Plage prévisionnelle" />
				</columns>
			</JFXTreeTableView>
			<HBox alignment="BASELINE_CENTER" prefHeight="80.0" prefWidth="800.0">
				<children>
					<JFXButton fx:id="boutonOuvrirPlan" buttonType="RAISED" onAction="#boutonOuvrirPlan" prefHeight="40" prefWidth="150.0" style="-fx-font-size:14px;-fx-background-color:WHITE;" text="Ouvrir plan">
						<cursor>
							<Cursor fx:constant="HAND" />
						</cursor>
						<font>
							<Font name="Arial" size="12.0" />
						</font>
						<HBox.margin>
							<Insets left="10.0" right="10.0" />
						</HBox.margin>
					</JFXButton>
					<JFXButton fx:id="boutonOuvrirLivraison" buttonType="RAISED" onAction="#boutonOuvrirLivraison" prefHeight="40" prefWidth="150.0" style="-fx-font-size:14px;-fx-background-color:WHITE;" text="Ouvrir livraison" visible="false">
						<cursor>
							<Cursor fx:constant="HAND" />
						</cursor>
						<font>
							<Font name="Arial" size="12.0" />
						</font>
						<HBox.margin>
							<Insets left="10.0" right="10.0" />
						</HBox.margin>
					</JFXButton>
					<JFXButton fx:id="boutonCalculerTournee" buttonType="RAISED" onAction="#boutonCalculerTournee" prefHeight="40" prefWidth="150.0" style="-fx-font-size:14px;-fx-background-color:WHITE;" text="Calculer tournée" visible="false">
						<cursor>
							<Cursor fx:constant="HAND" />
						</cursor>
						<font>
							<Font name="Arial" size="12.0" />
						</font>
						<HBox.margin>
							<Insets left="10.0" right="10.0" />
						</HBox.margin>
					</JFXButton>
					<JFXButton fx:id="boutonExporterTournee" buttonType="RAISED" onAction="#boutonExporterTournee" prefHeight="40" prefWidth="150.0" style="-fx-font-size:14px;-fx-background-color:WHITE;" text="Exporter tournée" visible="false">
						<cursor>
							<Cursor fx:constant="HAND" />
						</cursor>
						<font>
							<Font name="Arial" size="12.0" />
						</font>
						<HBox.margin>
							<Insets left="10.0" right="10.0" />
						</HBox.margin>
					</JFXButton>
				</children>
			</HBox>
		</children>
	</VBox>
</StackPane>
